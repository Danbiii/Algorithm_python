    "import datetime\n",
    "def weekday(a,b):\n",
    "    date = datetime.date(2016,a,b)\n",
    "    week = ['MON','TUE','WED','THU','FRI','SAT','SUN']\n",
    "    return week[date.weekday()]\n",
    "\n",
    "print(weekday(5,24))"
   ]
